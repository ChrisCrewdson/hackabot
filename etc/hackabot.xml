<hackabot>
	<directory>/home/manatee/hackabot</directory>
	<commands>commands</commands>
	<hooks>hooks</hooks>
	<etc>etc</etc>
	<socket>sock</socket>

	<server>irc.oregonstate.edu</server>
	<port>6667</port>
	<nick>manatee</nick>
	<name>Most certainly not the OSL logo!</name>

	<automsg to="nickserv" msg="identify beaverdamnit" />
	<automsg to="chanserv" msg="invite #osuosl-staff" />
	<automsg to="chanserv" msg="invite #foss-anonymous" />
	
	<autojoin chan="#osuosl" />
	<autojoin chan="#osuosl-staff" />
	<autojoin chan="#osuosl-infra" />
	<autojoin chan="#osu-lug" />
	<autojoin chan="#foss-anonymous" />
	<autojoin chan="#osu-chat" />
	<autojoin chan="#cws" />
	<autojoin chan="#maintain" />
	<autojoin chan="#maintain-dev" />
	<autojoin chan="#osu-cs411" />
	<autojoin chan="#manatee" msg="Now with 50% more seacow!" />

	<verbose>0</verbose>
	<cmdconfig>
		<topic>
			<topicmask chan="#osuosl" msg="OSUOSL :: " />
			<topicmask chan="#osuosl-staff" msg="OSUOSL :: " />
			<topicmask chan="#osuosl-infra" msg="OSUOSL :: " />
			<!--<topicmask chan="#manatee" msg="Manatee :: " />-->
		</topic>
	</cmdconfig>
</hackabot>
