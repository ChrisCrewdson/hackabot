def whiteindex():
	random.seed()
	return random.randint(0, len(whitecards)-1)

random.seed()
blackstring = blackcards[random.randint(0, len(blackcards)-1)]

hbc = Client()
msg = hbc.readline()

numargs = len(re.findall("{.}", blackstring))

if numargs == 0:
    info = "send No matches found. " + blackstring

elif numargs == 1:
    info = "send " + blackstring.format(whitecards[whiteindex()])

elif numargs == 2:
	info = "send " + blackstring.format(whitecards[whiteindex()], whitecards[whiteindex()])

elif numargs == 3:
    info = "send " + blackstring.format(whitecards[whiteindex()], whitecards[whiteindex()], whitecards[whiteindex()])

else:
    info = "send Too many args! " + numargs + blackstring

print info
